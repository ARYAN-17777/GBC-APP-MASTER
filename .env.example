# GBC Restaurant App - Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration (Production Backend)
EXPO_PUBLIC_SUPABASE_URL=your_supabase_project_url
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# JWT Configuration
JWT_SECRET=your_super_secure_jwt_secret_key_here
JWT_EXPIRES_IN=24h

# App Configuration
EXPO_PUBLIC_APP_ENV=development
EXPO_PUBLIC_API_BASE_URL=https://your-api-domain.com
EXPO_PUBLIC_APP_VERSION=1.0.0

# Security Configuration
EXPO_PUBLIC_MAX_LOGIN_ATTEMPTS=5
EXPO_PUBLIC_LOCKOUT_DURATION=900000
EXPO_PUBLIC_RATE_LIMIT_WINDOW=900000

# Currency Configuration
EXPO_PUBLIC_DEFAULT_CURRENCY=INR
EXPO_PUBLIC_CURRENCY_SYMBOL=â‚¹
EXPO_PUBLIC_MINOR_UNITS_PER_MAJOR=100

# Real-time Configuration
EXPO_PUBLIC_REALTIME_ENABLED=true
EXPO_PUBLIC_WEBSOCKET_URL=wss://your-websocket-domain.com

# Development/Testing
EXPO_PUBLIC_DEBUG_MODE=false
EXPO_PUBLIC_ENABLE_LOGGING=true
