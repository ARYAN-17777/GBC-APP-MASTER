    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-local-authentication": "~15.0.2",
    "expo-notifications": "~0.29.14",
    "expo-print": "~14.0.3",
    "expo-router": "~4.0.21",
