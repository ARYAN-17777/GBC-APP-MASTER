{
  "name": "gbc",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web --port 8081",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "mock-server": "node start-mock-server.js",
    "dev": "concurrently \"npm run mock-server\" \"npm run start\"",
    "build:dev": "eas build --profile development --platform android",
    "build:preview": "npm run pre-build-check && eas build --profile preview --platform android",
    "build:prod": "npm run pre-build-check && eas build --profile production --platform android",
    "build:all": "npm run pre-build-check && eas build --profile preview --platform all",
    "pre-build-check": "ts-node scripts/pre-build-check.ts",
    "test:imin": "ts-node scripts/test-imin-printer.ts"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@expo-google-fonts/outfit": "^0.3.0",
    "@expo/metro-runtime": "~4.0.1",
    "@expo/vector-icons": "~14.0.4",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@supabase/supabase-js": "^2.58.0",
    "axios": "^1.12.1",
    "bcryptjs": "^3.0.2",
    "concurrently": "^9.2.1",
